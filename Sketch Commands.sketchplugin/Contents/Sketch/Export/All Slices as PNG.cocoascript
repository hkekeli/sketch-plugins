// Export all slices
// @shortcut ctrl alt shift x

@import '../library/common.js'

var onRun = function(context) {
  var doc = context.document

  if ([doc fileURL] == null) {
    alert("You need to save your document to use this command");
  } else {
    com.bomberstudios.export_all_slices("png");
  }
}
